<main class="mainCRegister fondoWhite">
    <section class="sectionCRegister fondoWhite">
        <article class="articleRegister">
            <div class="divRegister">
                <h2 class="tituloRegister lblRWhite">Registro</h2>
                <p class="txtRegister lblRWhite">Regístrate para disfrutar de Chatbot</p>
                <form class="formRegister">
                    <div class="cInputRegister fondoWhite">
                        <input class="inputRegisterWhite borderWhite" type="text" name="nombre" id="nombre">
                        <label class="labelRegister fondoWhite lblRWhite" for="nombre">Nombre</label>
                    </div>
                    <div class="cInputRegister fondoWhite">
                        <input class="inputRegisterWhite borderWhite inputDateDark" type="date" name="fechaDeNacimiento" id="fechaDeNacimiento">
                        <label class="labelRegister fondoWhite lblRWhite" for="fechaDeNacimiento">Fecha de Nacimiento</label>
                    </div>
                    <div class="cInputRegister fondoWhite">
                        <input class="inputRegisterWhite borderWhite" type="email" name="email" id="email">
                        <label class="labelRegister fondoWhite lblRWhite" for="email">Email</label>
                    </div>
                    <div class="cInputRegister fondoWhite">
                        <input class="inputRegisterWhite borderWhite" type="password" name="contraseña" id="contraseña">
                        <label class="labelRegister fondoWhite lblRWhite" for="contraseña">Contraseña</label>
                        <button class="btnContra lblRWhite"><i class="fa-solid fa-eye"></i></button>
                    </div>
                    <button class="btnRegistrarse" type="submit">Registrarse</button>
                    <div class="cOr">
                        <div class="lineaOr lineaWhite"></div>
                        <span class="or lblRWhite">O</span>
                        <div class="lineaOr lineaWhite"></div>
                    </div>
                    <div class="cBtnGoogle">
                        <button class="btnGoogle modoWhite shadowColorWhite"><span class="txtGmail">Continuar con Google</span> <img class="imgGmail" src="/assets/img/gmail.png" alt="gmail"></button>
                    </div>
                    <p class="txtIniciarSesion lblRWhite">¿Ya tiene una cuenta? <a class="lIniciarSesionDark" href="#">Iniciar sesión</a></p>
                </form>
            </div>
        </article>
        <article class="articleFRegister">
        </article>
    </section>
</main>